{
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@app/*": [
        "app/*"
      ],
      "@store/*": [
        "app/store/*"
      ]
    },
    "skipLibCheck": true,
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "types": [
      "jquery"
    ]
  }
}
